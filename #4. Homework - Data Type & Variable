'''
#1. Homework: Math operations

a = float(input('Enter first number:'))
b = float(input('Enter second number: '))
c = a + b
d = a - b
e = a * b
f = a / b

print(f'{a} + {b} = {c}')
print(f'{a} - {b} = {d}')
print(f'{a} * {b} = {e}')
print(f'{a} / {b} = {f}')


#2. Homework: Student grades

first_student_name = str(input("Enter the first student's name: "))
first_student_id = str(input("Enter the first student's ID: "))
first_student_grade = float(input("Enter the first student's grade: "))

print('\n')

second_student_name = str(input("Enter the second student's name: "))
second_student_id = str(input("Enter the second student's ID: "))
second_student_grade = float(input("Enter the second student's grade: "))

print('\n')

average_grade = (first_student_grade + second_student_grade)/2

print(f'{first_student_name}(ID {first_student_id}) got grade: {first_student_grade}')
print(f'{second_student_name}(ID {second_student_id}) got grade: {second_student_grade}')
print(f'Average math grades is {average_grade}')
'''

#3. Homework: Even and Odd Sum

a, b, c, d, e, f, g, h = map(int, input('Input: ').split())
output1, output2 = a + c + e + g, b + d + f + h

print(f'Output: {output2} {output1}')
