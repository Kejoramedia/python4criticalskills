#3. Homework: Even and Odd Sum

a, b, c, d, e, f, g, h = map(int, input('Input: ').split())
output1, output2 = a + c + e + g, b + d + f + h

print(f'Output: {output2} {output1}')


#4. Homework: Special concatenation
a = str(input(''))
b = str(input(''))
c = str(input(''))
print(f'''{a}'{b}"{c}'''*10)

