
# EASY CHALLENGE
#1. Homework: Math operations
a = float(input('Enter first number:'))
b = float(input('Enter second number: '))

print(f'{a} + {b} = {a+b}')
print(f'{a} - {b} = {a-b}')
print(f'{a} * {b} = {a*b}')
print(f'{a} / {b} = {a/b}')


